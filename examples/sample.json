{
    "config": { "file": "examples/rust.json" },
    
    "var": { "type": "void*",
             "name": "void_ptr",
             "qualifier": "extern"
    },
    "enum": {
      "name": "FooEnum",
      "attribute": "C-ABI",
      "var": [
        { "name": "EnumVal1", "type": "int" },
        { "name": "EnumVal2", "type": "int" },
        { "name": "EnumVal3", "type": "int" },
        { "name": "Count", "value": "3", "type": "int" }
      ]
    },
    "struct": {
      "name": "Foo",
      "attribute": "C-ABI",
      "var": [
        { "type": "int", "name": "x" },
        { "type": "int", "name": "y" }
      ],
      "func": [
        {
          "type": "char",
          "name": "return_arg",
          "var": {
            "type": "char",
            "name": "arg"
          }
        },
        {
          "type": "void (*sample_ptr)",
          "var": [
            { "type": "const char*", "name": "fmt" },
            { "type": "..." }
          ]
        }
      ],
      "struct": {
            "name": "Bar",
            "enum": {
                "name": "BarEnum",
                "var": [
                    { "name": "EnumVal4" },
                    { "name": "EnumVal5" }
                ]
            },
      "func": {
          "qualifier": "extern",
          "type": "int",
          "name": "foo",
          "var": {
            "type": "int",
            "name": "x",
            "value": "1"
          }
        }
      }
    },
    "some_item": { "type": "whatever",
                   "name": "unsupported"
    }
}